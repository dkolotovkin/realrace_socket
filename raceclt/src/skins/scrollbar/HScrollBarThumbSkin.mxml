<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark">
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<s:Rect left="0" right="0" top="0" bottom="0" minWidth="14">
		<s:stroke>
			<s:SolidColorStroke color="0x666666"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="0xaaaaaa" alpha="1"/>
		</s:fill>
	</s:Rect>	
</s:SparkSkin>
